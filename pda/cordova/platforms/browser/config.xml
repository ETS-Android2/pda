<?xml version='1.0' encoding='utf-8'?>
<widget id="famo.pda" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FAMO - PDA</name>
    <description>
        An application made by FAMO that is going to replace old PDA's.
    </description>
    <author email="servicedesk@famo.pt" href="http://www.famo.pt/pt/">
        FAMO
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
